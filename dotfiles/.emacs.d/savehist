    branches: [ main ]
  workflow_dispatch:

jobs:
  build-windows-installer:
    runs-on: windows-latest
    env:
      NODE_VERSION: \"24.10.0\"
    
    steps:
      - name: Checkout repository
        uses: actions/checkout@v4

      - name: Setup Node.js
        uses: actions/setup-node@v4
        with:
          node-version: ${{ env.NODE_VERSION }}

      - name: Install dependencies
        run: npm install

      - name: Build Next.js
        run: npm run build

      - name: Verify build (Windows)
        if: runner.os == 'Windows'
        shell: pwsh
        run: |
          Get-ChildItem .next -Recurse | Select-Object FullName
          if (Test-Path .next/BUILD_ID) {
            Write-Host \"Build ID:\"
            Get-Content .next/BUILD_ID
          } else {
            Write-Host \"BUILD_ID not found!\"
          }

      - name: Verify build (Linux)
        if: runner.os == 'Linux'
        run: |
          ls -la .next/
          echo \"Build ID:\"
          cat .next/BUILD_ID || echo \"BUILD_ID not found\"

      - name: Build Neutralino
        run: |
          npx neu update
          npx neu build

      - name: Initialize database
        run: |
          if (!(Test-Path \"data/pos.db\")) {
            npm run db:init
          }
        shell: pwsh

      - name: Download Node.js portable
        shell: pwsh
        run: |
          $nodeUrl = \"https://nodejs.org/dist/v$env:NODE_VERSION/node-v$env:NODE_VERSION-win-x64.zip\"
          Invoke-WebRequest $nodeUrl -OutFile \"$env:TEMP\\node-portable.zip\"
          Expand-Archive \"$env:TEMP\\node-portable.zip\" -DestinationPath \"$env:TEMP\\node-portable\"
          Move-Item \"$env:TEMP\\node-portable\\node-v$env:NODE_VERSION-win-x64\" \"$env:GITHUB_WORKSPACE\\node-portable\"

      - name: Create package structure
        shell: pwsh
        run: |
          New-Item -ItemType Directory -Force -Path \"installer-package\"
          
          # Copiar aplicação completa
          Copy-Item -Recurse -Force \".next\" \"installer-package\\.next\"
          Copy-Item -Recurse -Force \"dist\" \"installer-package\\dist\"
          Copy-Item -Recurse -Force \"data\" \"installer-package\\data\"
          Copy-Item -Recurse -Force \"public\" \"installer-package\\public\"
          Copy-Item -Recurse -Force \"node_modules\" \"installer-package\\node_modules\"
          Copy-Item -Recurse -Force \"node-portable\" \"installer-package\\node-portable\"
          
          # Copiar arquivos de configuração e scripts
          Copy-Item \"package.json\" \"installer-package\\\"
          Copy-Item \"package-lock.json\" \"installer-package\\\"
          Copy-Item \"next.config.ts\" \"installer-package\\\"
          Copy-Item \"neutralino.config.json\" \"installer-package\\\"
          
          # Copiar scripts personalizados se existirem
          if (Test-Path \"scripts\") {
            Copy-Item -Recurse -Force \"scripts\" \"installer-package\\scripts\"
          }
          
          # Criar script de inicialização
          @'
          @echo off
          set NODE_PATH=%~dp0node-portable
          set PATH=%NODE_PATH%;%PATH%
          cd /d \"%~dp0\"
          start \"\" \"%NODE_PATH%\\node.exe\" run-pos.js
          '@ | Out-File -Encoding ASCII \"installer-package\\POS-System.bat\"
          
          # Copiar run-pos.js se existir, senão criar um básico
          if (Test-Path \"run-pos.js\") {
            Copy-Item \"run-pos.js\" \"installer-package\\\"
          } else {
            @'
          const { spawn } = require('child_process');
          const path = require('path');
          
          const server = spawn('npm', ['run', 'start'], { 
            shell: true, 
            stdio: 'inherit' 
          });
          
          setTimeout(() => {
            const neuBin = path.join(__dirname, 'dist', 'pos-system', 'pos-system-win_x64.exe');
            spawn(neuBin, [], { shell: true, stdio: 'inherit' });
          }, 5000);
          '@ | Out-File -Encoding UTF8 \"installer-package\\run-pos.js\"
          }

      - name: Install NSIS
        run: choco install nsis -y
        shell: cmd

      - name: Create NSIS installer script
        shell: pwsh
        run: |
          $nsisScript = @'
          !include \"MUI2.nsh\"
          
          Name \"POS System\"
          OutFile \"POSSystemInstaller.exe\"
          InstallDir \"$PROGRAMFILES64\\POS System\"
          RequestExecutionLevel admin
          
          !define MUI_ABORTWARNING
          !define MUI_ICON \"${NSISDIR}\\Contrib\\Graphics\\Icons\\modern-install.ico\"
          
          !insertmacro MUI_PAGE_WELCOME
          !insertmacro MUI_PAGE_DIRECTORY
          !insertmacro MUI_PAGE_INSTFILES
          !insertmacro MUI_PAGE_FINISH
          
          !insertmacro MUI_LANGUAGE \"English\"
          
          Section \"Install\"
            SetOutPath \"$INSTDIR\"
            
            File /r \"installer-package\\*.*\"
            
            CreateShortCut \"$DESKTOP\\POS System.lnk\" \"$INSTDIR\\POS-System.bat\" \"\" \"$INSTDIR\\dist\\pos-system\\resources\\icons\\appIcon.png\"
            CreateShortCut \"$SMPROGRAMS\\POS System.lnk\" \"$INSTDIR\\POS-System.bat\" \"\" \"$INSTDIR\\dist\\pos-system\\resources\\icons\\appIcon.png\"
            
            WriteUninstaller \"$INSTDIR\\Uninstall.exe\"
            
            WriteRegStr HKLM \"Software\\Microsoft\\Windows\\CurrentVersion\\Uninstall\\POSSystem\" \"DisplayName\" \"POS System\"
            WriteRegStr HKLM \"Software\\Microsoft\\Windows\\CurrentVersion\\Uninstall\\POSSystem\" \"UninstallString\" \"$INSTDIR\\Uninstall.exe\"
          SectionEnd
          
          Section \"Uninstall\"
            Delete \"$DESKTOP\\POS System.lnk\"
            Delete \"$SMPROGRAMS\\POS System.lnk\"
            RMDir /r \"$INSTDIR\"
            DeleteRegKey HKLM \"Software\\Microsoft\\Windows\\CurrentVersion\\Uninstall\\POSSystem\"
          SectionEnd
          '@
          
          $nsisScript | Out-File -Encoding ASCII \"installer.nsi\"

      - name: Build NSIS installer
        shell: cmd
        run: |
          \"C:\\Program Files (x86)\\NSIS\\makensis.exe\" installer.nsi

      - name: Upload Windows installer
        uses: actions/upload-artifact@v4
        with:
          name: POSSystemInstaller-Windows
          path: POSSystemInstaller.exe

  build-linux-installer:
    runs-on: ubuntu-latest
    container:
      image: node:24-bookworm
    env:
      NODE_VERSION: \"24.10.0\"
    
    steps:
      - name: Checkout repository
        uses: actions/checkout@v4

      - name: Setup Node.js
        uses: actions/setup-node@v4
        with:
          node-version: ${{ env.NODE_VERSION }}

      - name: Install dependencies
        run: npm install

      - name: Build Next.js
        run: npm run build

      - name: Build Neutralino
        run: |
          npx neu update
          npx neu build

      - name: Initialize database
        run: |
          if [ ! -f \"data/pos.db\" ]; then
            npm run db:init
          fi

      - name: Create Linux package structure
        run: |
          mkdir -p linux-package/opt/pos-system
          
          # Verificar se .next existe e tem conteúdo
          if [ ! -d \".next\" ] || [ ! -f \".next/BUILD_ID\" ]; then
            echo \"Error: .next build not found or incomplete!\"
            exit 1
          fi
          
          # Copiar aplicação completa
          cp -r .next linux-package/opt/pos-system/
          cp -r dist linux-package/opt/pos-system/
          cp -r data linux-package/opt/pos-system/
          cp -r public linux-package/opt/pos-system/
          cp -r node_modules linux-package/opt/pos-system/
          
          # Copiar arquivos de configuração
          cp package.json linux-package/opt/pos-system/
          cp package-lock.json linux-package/opt/pos-system/
          cp next.config.ts linux-package/opt/pos-system/
          cp next.config.mjs linux-package/opt/pos-system/ 2>/dev/null || true
          cp next.config.js linux-package/opt/pos-system/ 2>/dev/null || true
          cp neutralino.config.json linux-package/opt/pos-system/
          
          # Copiar scripts se existirem
          if [ -d \"scripts\" ]; then
            cp -r scripts linux-package/opt/pos-system/
          fi
          
          # Verificar o que foi copiado
          echo \"Verificando .next copiado:\"
          ls -la linux-package/opt/pos-system/.next/
          
          # Criar script de inicialização
          cat > linux-package/opt/pos-system/pos-system.sh << 'EOF'
          #!/bin/bash
          DIR=\"$( cd \"$( dirname \"${BASH_SOURCE[0]}\" )\" && pwd )\"
          cd \"$DIR\"
          
          # Verificar se .next existe
          if [ ! -d \".next\" ]; then
            echo \"Error: .next directory not found!\"
            exit 1
          fi
          
          # Iniciar servidor Next.js em background
          npm run start &
          SERVER_PID=$!
          
          # Aguardar servidor estar pronto
          sleep 5
          
          # Iniciar Neutralino
          ./dist/pos-system/pos-system-linux_x64
          
          # Quando Neutralino fechar, matar servidor
          kill $SERVER_PID
          EOF
          
          chmod +x linux-package/opt/pos-system/pos-system.sh

      - name: Create DEB package
        run: |
          mkdir -p linux-package/DEBIAN
          mkdir -p linux-package/usr/share/applications
          mkdir -p linux-package/usr/share/icons/hicolor/256x256/apps
          mkdir -p linux-package/usr/local/bin
          
          # Criar control file
          cat > linux-package/DEBIAN/control << EOF
          Package: pos-system
          Version: 1.0.0
          Section: utils
          Priority: optional
          Architecture: amd64
          Depends: nodejs (>= 18.0.0)
          Maintainer: POS System <contact@possystem.com>
          Description: POS System
           Complete Point of Sale system with Next.js and Neutralino
          EOF
          
          # Criar desktop entry
          cat > linux-package/usr/share/applications/pos-system.desktop << EOF
          [Desktop Entry]
          Name=POS System
          Exec=/opt/pos-system/pos-system.sh
          Icon=pos-system
          Type=Application
          Categories=Office;Finance;
          Terminal=false
          EOF
          
          # Criar symlink para executar de qualquer lugar
          mkdir -p linux-package/usr/local/bin
          ln -s /opt/pos-system/pos-system.sh linux-package/usr/local/bin/pos-system
          
          # Copiar ícone se existir
          if [ -f \"public/icon.png\" ]; then
            cp public/icon.png linux-package/usr/share/icons/hicolor/256x256/apps/pos-system.png
          elif [ -f \"dist/pos-system/resources/icons/appIcon.png\" ]; then
            cp dist/pos-system/resources/icons/appIcon.png linux-package/usr/share/icons/hicolor/256x256/apps/pos-system.png
          fi
          
          # Criar pacote
          dpkg-deb --build linux-package pos-system_1.0.0_amd64.deb

      - name: Install AppImage tools
        run: |
          apt-get update
          apt-get install -y fuse libfuse2 imagemagick wget file fonts-liberation

      - name: Create AppImage
        run: |
          wget -O appimagetool https://github.com/AppImage/AppImageKit/releases/download/continuous/appimagetool-x86_64.AppImage
          chmod +x appimagetool
          
          mkdir -p AppDir/usr/bin
          mkdir -p AppDir/usr/share/applications
          mkdir -p AppDir/usr/share/icons/hicolor/256x256/apps
          
          cp -r linux-package/opt/pos-system/* AppDir/usr/bin/
          cp linux-package/usr/share/applications/pos-system.desktop AppDir/
          cp linux-package/usr/share/applications/pos-system.desktop AppDir/usr/share/applications/
          
          if [ -f \"linux-package/usr/share/icons/hicolor/256x256/apps/pos-system.png\" ]; then
            cp linux-package/usr/share/icons/hicolor/256x256/apps/pos-system.png AppDir/pos-system.png
            cp linux-package/usr/share/icons/hicolor/256x256/apps/pos-system.png AppDir/usr/share/icons/hicolor/256x256/apps/
          else
            convert -size 256x256 xc:#4F46E5 -font Liberation-Sans -pointsize 72 -fill white -gravity center -annotate +0+0 \"POS\" AppDir/pos-system.png
            cp AppDir/pos-system.png AppDir/usr/share/icons/hicolor/256x256/apps/pos-system.png
          fi
          
          cat > AppDir/AppRun << 'EOF'
          #!/bin/bash
          SELF=$(readlink -f \"$0\")
          HERE=${SELF%/*}
          export PATH=\"${HERE}/usr/bin:${PATH}\"
          cd \"${HERE}/usr/bin\"
          exec \"${HERE}/usr/bin/pos-system.sh\" \"$@\"
          EOF
          chmod +x AppDir/AppRun
          
          # Extrair e executar sem FUSE
          ./appimagetool --appimage-extract
          ARCH=x86_64 ./squashfs-root/AppRun AppDir POSSystem-x86_64.AppImage

      - name: Upload Linux DEB
        uses: actions/upload-artifact@v4
        with:
          name: POSSystem-Linux-DEB
          path: pos-system_1.0.0_amd64.deb

      - name: Upload Linux AppImage
        uses: actions/upload-artifact@v4
        with:
          name: POSSystem-Linux-AppImage
          path: POSSystem-x86_64.AppImage

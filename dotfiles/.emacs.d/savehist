(let ((custom-paths '(\"/home/pedro/.cabal/bin\"
                       \"/home/pedro/.local/bin\"
                       \"/home/pedro/.local/share/mise/shims\")))
  (setenv \"PATH\" (concat (mapconcat 'identity custom-paths \":\") \":\" (getenv \"PATH\")))
  (setq exec-path (append custom-paths exec-path)))


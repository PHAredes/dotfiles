;; ============= Unicode/LaTeX Math Input =============
(use-package math-symbol-lists
  :straight t)

(defun my/math-symbol-completion ()
  \"Completion backend for LaTeX math symbols.\"
  (when (and (or (derived-mode-p 'prog-mode 'text-mode)
                 (minibufferp))
             (looking-back \"\\\\\\\\[a-zA-Z]*\" (- (point) 20)))
    (let* ((prefix (match-string 0))
           (bounds (cons (match-beginning 0) (match-end 0)))
           (candidates
            (mapcar (lambda (symbol)
                      (cons (concat \"\\\\\" (car symbol))
                            (cdr symbol)))
                    (append math-symbol-list-basic
                            math-symbol-list-extended))))
      (list (car bounds)
            (cdr bounds)
            (completion-table-dynamic
             (lambda (_)
               (all-completions prefix candidates)))
            :annotation-function
            (lambda (candidate)
              (let ((unicode (cdr (assoc candidate candidates))))
                (when unicode
                  (format \" → %s\" unicode))))
            :exit-function
            (lambda (candidate _status)
              (let ((unicode (cdr (assoc candidate candidates))))
                (when unicode
                  (delete-region (- (point) (length candidate)) (point))
                  (insert unicode))))))))

(add-to-list 'completion-at-point-functions #'my/math-symbol-completion)

;; Keybinding para forçar completion de math symbols
(global-set-key (kbd \"C-c m\") #'completion-at-point)


on:
  push:
    branches: [ main ]
  workflow_dispatch:

jobs:
  build-windows-installer:
    runs-on: windows-latest
    env:
      NODE_VERSION: \"24.10.0\"
    
    steps:
      - name: Checkout repository
        uses: actions/checkout@v4

      - name: Setup Node.js
        uses: actions/setup-node@v4
        with:
          node-version: ${{ env.NODE_VERSION }}

      - name: Install dependencies
        run: npm install

      - name: Build Next.js
        run: npm run build

      - name: Verify build (Windows)
        if: runner.os == 'Windows'
        shell: pwsh
        run: |
          Get-ChildItem .next -Recurse | Select-Object FullName
          if (Test-Path .next/BUILD_ID) {
            Write-Host \"Build ID:\"
            Get-Content .next/BUILD_ID
          } else {
            Write-Host \"BUILD_ID not found!\"
          }

      - name: Verify build (Linux)
        if: runner.os == 'Linux'
        run: |
          ls -la .next/
          echo \"Build ID:\"
          cat .next/BUILD_ID || echo \"BUILD_ID not found\"

      - name: Build Neutralino
        run: |
          npx neu update
          npx neu build

      - name: Initialize database
        run: |
          if (!(Test-Path \"data/pos.db\")) {
            npm run db:init
          }
        shell: pwsh

      - name: Download Node.js portable
        shell: pwsh
        run: |
          $nodeUrl = \"https://nodejs.org/dist/v$env:NODE_VERSION/node-v$env:NODE_VERSION-win-x64.zip\"
          Invoke-WebRequest $nodeUrl -OutFile \"$env:TEMP\\node-portable.zip\"
          Expand-Archive \"$env:TEMP\\node-portable.zip\" -DestinationPath \"$env:TEMP\\node-portable\"
          Move-Item \"$env:TEMP\\node-portable\\node-v$env:NODE_VERSION-win-x64\" \"$env:GITHUB_WORKSPACE\\node-portable\"

      - name: Create package structure
        shell: pwsh
        run: |
          New-Item -ItemType Directory -Force -Path \"installer-package\"
          
          # Copiar aplicação completa
          Copy-Item -Recurse -Force \".next\" \"installer-package\\.next\"
          Copy-Item -Recurse -Force \"dist\" \"installer-package\\dist\"
          Copy-Item -Recurse -Force \"data\" \"installer-package\\data\"
          Copy-Item -Recurse -Force \"public\" \"installer-package\\public\"
          Copy-Item -Recurse -Force \"node_modules\" \"installer-package\\node_modules\"
          Copy-Item -Recurse -Force \"node-portable\" \"installer-package\\node-portable\"
          
          # Copiar arquivos de configuração e scripts
          Copy-Item \"package.json\" \"installer-package\\\"
          Copy-Item \"package-lock.json\" \"installer-package\\\"
          Copy-Item \"next.config.ts\" \"installer-package\\\"
          Copy-Item \"neutralino.config.json\" \"installer-package\\\"
          
          # Copiar scripts personalizados se existirem
          if (Test-Path \"scripts\") {
            Copy-Item -Recurse -Force \"scripts\" \"installer-package\\scripts\"
          }
          
          # Criar script de inicialização
          @'
          @echo off
          set NODE_PATH=%~dp0node-portable
          set PATH=%NODE_PATH%;%PATH%
          cd /d \"%~dp0\"
          start \"\" \"%NODE_PATH%\\node.exe\" run-pos.js
          '@ | Out-File -Encoding ASCII \"installer-package\\POS-System.bat\"
          
          # Copiar run-pos.js se existir, senão criar um básico
          if (Test-Path \"run-pos.js\") {
            Copy-Item \"run-pos.js\" \"installer-package\\\"
          } else {
            @'
          const { spawn } = require('child_process');
          const path = require('path');
          
          const server = spawn('npm', ['run', 'start'], { 
            shell: true, 
            stdio: 'inherit' 
          });
          
          setTimeout(() => {
            const neuBin = path.join(__dirname, 'dist', 'pos-system', 'pos-system-win_x64.exe');
            spawn(neuBin, [], { shell: true, stdio: 'inherit' });
          }, 5000);
          '@ | Out-File -Encoding UTF8 \"installer-package\\run-pos.js\"
          }

      - name: Install NSIS
        run: choco install nsis -y
        shell: cmd

      - name: Create NSIS installer script
        shell: pwsh
        run: |
          $nsisScript = @'
          !include \"MUI2.nsh\"
          
          Name \"POS System\"
          OutFile \"POSSystemInstaller.exe\"
          InstallDir \"$PROGRAMFILES64\\POS System\"
          RequestExecutionLevel admin
          
          !define MUI_ABORTWARNING
          !define MUI_ICON \"${NSISDIR}\\Contrib\\Graphics\\Icons\\modern-install.ico\"
          
          !insertmacro MUI_PAGE_WELCOME
          !insertmacro MUI_PAGE_DIRECTORY
          !insertmacro MUI_PAGE_INSTFILES
          !insertmacro MUI_PAGE_FINISH
          
          !insertmacro MUI_LANGUAGE \"English\"
          
          Section \"Install\"
            SetOutPath \"$INSTDIR\"
            
            File /r \"installer-package\\*.*\"
            
            CreateShortCut \"$DESKTOP\\POS System.lnk\" \"$INSTDIR\\POS-System.bat\" \"\" \"$INSTDIR\\dist\\pos-system\\resources\\icons\\appIcon.png\"
            CreateShortCut \"$SMPROGRAMS\\POS System.lnk\" \"$INSTDIR\\POS-System.bat\" \"\" \"$INSTDIR\\dist\\pos-system\\resources\\icons\\appIcon.png\"
            
            WriteUninstaller \"$INSTDIR\\Uninstall.exe\"
            
            WriteRegStr HKLM \"Software\\Microsoft\\Windows\\CurrentVersion\\Uninstall\\POSSystem\" \"DisplayName\" \"POS System\"
            WriteRegStr HKLM \"Software\\Microsoft\\Windows\\CurrentVersion\\Uninstall\\POSSystem\" \"UninstallString\" \"$INSTDIR\\Uninstall.exe\"
          SectionEnd
          
          Section \"Uninstall\"
            Delete \"$DESKTOP\\POS System.lnk\"
            Delete \"$SMPROGRAMS\\POS System.lnk\"
            RMDir /r \"$INSTDIR\"
            DeleteRegKey HKLM \"Software\\Microsoft\\Windows\\CurrentVersion\\Uninstall\\POSSystem\"
          SectionEnd
          '@
          
          $nsisScript | Out-File -Encoding ASCII \"installer.nsi\"

      - name: Build NSIS installer
        shell: cmd
        run: |
          \"C:\\Program Files (x86)\\NSIS\\makensis.exe\" installer.nsi

      - name: Upload Windows installer
        uses: actions/upload-artifact@v4
        with:
          name: POSSystemInstaller-Windows
          path: POSSystemInstaller.exe


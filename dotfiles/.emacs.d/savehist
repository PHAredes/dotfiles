;; Haskell
(use-package haskell-mode
  :straight t
  :hook
  ((haskell-mode . interactive-haskell-mode)
   (haskell-mode . turn-on-haskell-doc-mode)
   (haskell-mode . turn-on-haskell-indentation)))

;; TypeScript/JavaScript

(use-package eev
  :straight t
  :demand t
  :config
  (require 'eev-load)
  (require 'eev-aliases)
  (eev-mode 1)
  
  ;; Make the eev keymap take precedence over Evil's keymap
  (evil-make-overriding-map eev-mode-map 'normal)
  

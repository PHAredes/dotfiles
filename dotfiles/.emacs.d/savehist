(defvar my/math-symbols
  '((\"\\\\alpha\" . \"α\") (\"\\\\beta\" . \"β\") (\"\\\\gamma\" . \"γ\") (\"\\\\delta\" . \"δ\")
    (\"\\\\epsilon\" . \"ε\") (\"\\\\zeta\" . \"ζ\") (\"\\\\eta\" . \"η\") (\"\\\\theta\" . \"θ\")
    (\"\\\\iota\" . \"ι\") (\"\\\\kappa\" . \"κ\") (\"\\\\lambda\" . \"λ\") (\"\\\\mu\" . \"μ\")
    (\"\\\\nu\" . \"ν\") (\"\\\\xi\" . \"ξ\") (\"\\\\pi\" . \"π\") (\"\\\\rho\" . \"ρ\")
    (\"\\\\sigma\" . \"σ\") (\"\\\\tau\" . \"τ\") (\"\\\\upsilon\" . \"υ\") (\"\\\\phi\" . \"φ\")
    (\"\\\\chi\" . \"χ\") (\"\\\\psi\" . \"ψ\") (\"\\\\omega\" . \"ω\")
    (\"\\\\Alpha\" . \"Α\") (\"\\\\Beta\" . \"Β\") (\"\\\\Gamma\" . \"Γ\") (\"\\\\Delta\" . \"Δ\")
    (\"\\\\Epsilon\" . \"Ε\") (\"\\\\Zeta\" . \"Ζ\") (\"\\\\Eta\" . \"Η\") (\"\\\\Theta\" . \"Θ\")
    (\"\\\\Iota\" . \"Ι\") (\"\\\\Kappa\" . \"Κ\") (\"\\\\Lambda\" . \"Λ\") (\"\\\\Mu\" . \"Μ\")
    (\"\\\\Nu\" . \"Ν\") (\"\\\\Xi\" . \"Ξ\") (\"\\\\Pi\" . \"Π\") (\"\\\\Rho\" . \"Ρ\")
    (\"\\\\Sigma\" . \"Σ\") (\"\\\\Tau\" . \"Τ\") (\"\\\\Upsilon\" . \"Υ\") (\"\\\\Phi\" . \"Φ\")
    (\"\\\\Chi\" . \"Χ\") (\"\\\\Psi\" . \"Ψ\") (\"\\\\Omega\" . \"Ω\")
    (\"\\\\to\" . \"→\") (\"\\\\rarr\" . \"→\") (\"\\\\larr\" . \"←\") (\"\\\\uarr\" . \"↑\") (\"\\\\darr\" . \"↓\")
    (\"\\\\leftrightarrow\" . \"↔\") (\"\\\\Rightarrow\" . \"⇒\") (\"\\\\Leftarrow\" . \"⇐\")
    (\"\\\\forall\" . \"∀\") (\"\\\\exists\" . \"∃\") (\"\\\\nexists\" . \"∄\")
    (\"\\\\in\" . \"∈\") (\"\\\\notin\" . \"∉\") (\"\\\\ni\" . \"∋\")
    (\"\\\\emptyset\" . \"∅\") (\"\\\\subset\" . \"⊂\") (\"\\\\supset\" . \"⊃\")
    (\"\\\\subseteq\" . \"⊆\") (\"\\\\supseteq\" . \"⊇\")
    (\"\\\\cup\" . \"∪\") (\"\\\\cap\" . \"∩\") (\"\\\\times\" . \"×\") (\"\\\\cdot\" . \"⋅\")
    (\"\\\\sum\" . \"∑\") (\"\\\\prod\" . \"∏\") (\"\\\\int\" . \"∫\")
    (\"\\\\leq\" . \"≤\") (\"\\\\geq\" . \"≥\") (\"\\\\neq\" . \"≠\") (\"\\\\approx\" . \"≈\")
    (\"\\\\equiv\" . \"≡\") (\"\\\\sim\" . \"∼\") (\"\\\\infty\" . \"∞\")
    (\"\\\\partial\" . \"∂\") (\"\\\\nabla\" . \"∇\") (\"\\\\neg\" . \"¬\")
    (\"\\\\wedge\" . \"∧\") (\"\\\\vee\" . \"∨\") (\"\\\\oplus\" . \"⊕\") (\"\\\\otimes\" . \"⊗\"))
  \"LaTeX to Unicode math symbol mappings.\")


(defun my/math-symbol-completion ()
  \"Completion backend for LaTeX math symbols.\"
  (when (looking-back \"\\\\\\\\[a-zA-Z]*\" (line-beginning-position))
    (let* ((end (point))
           (start (match-beginning 0))
           (prefix (buffer-substring-no-properties start end)))
      (list start end
            (all-completions prefix my/math-symbols)
            :annotation-function
            (lambda (cand)
              (concat \"   \" (cdr (assoc cand my/math-symbols))))
            :exit-function
            (lambda (cand _status)
              (let ((unicode (cdr (assoc cand my/math-symbols))))
                (when unicode
                  (delete-region start end)
                  (insert unicode))))))))

;; IMPORTANTE: adiciona no início da lista (prioridade)
(add-hook 'after-init-hook
          (lambda ()
            (add-to-list 'completion-at-point-functions #'my/math-symbol-completion)))

;; Keybinding para forçar completion
(global-set-key (kbd \"C-c m\") #'completion-at-point)

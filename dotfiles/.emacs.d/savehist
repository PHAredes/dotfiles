  (define-key evil-motion-state-map (kbd \"RET\") nil)
  (define-key evil-motion-state-map (kbd \" \") nil)
  (define-key evil-visual-state-map (kbd \" \") nil)
  (define-key evil-insert-state-map (kbd \" \") nil))


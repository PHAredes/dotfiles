  (corfu-auto t)
  (corfu-auto-delay 0.2)
  (corfu-auto-prefix 2)
  (corfu-quit-no-match 'separator)
  (corfu-preview-current nil)
  (corfu-preselect 'prompt)
  :bind (:map corfu-map
              (\"TAB\" . corfu-next)
              ([tab] . corfu-next)
              (\"S-TAB\" . corfu-previous)
              ([backtab] . corfu-previous))
  :init
  (global-corfu-mode))


          name: POSSystem-Linux-DEB
          path: pos-system_1.0.0_amd64.deb

      - name: Upload Linux AppImage
        uses: actions/upload-artifact@v4
        with:
          name: POSSystem-Linux-AppImage
          path: POSSystem-x86_64.AppImage
